<h1>Our Cocktails</h1>

<div class="container">

  <div class="cards">
    <% @cocktails.each do |cocktail| %>
      <% if cocktail.photo.url %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path(cocktail.photo) %>)">
      <% else %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,)">
      <% end %>
      <%= link_to cocktail.name.capitalize, cocktail_path(cocktail) %>
        </div>
    <% end %>
  </div>
</div>
